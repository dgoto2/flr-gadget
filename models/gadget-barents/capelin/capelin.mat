;
;	Base file for mature (age 4+) capelin
;
stockname              capelin.mat
livesonareas           1 
minage                 4
maxage                 5; a plus group
minlength              0
maxlength              25; a plus group
dl                     0.5; step size for the length groups
refweightfile          capelin/capelin.refw
growthandeatlengths    agg/capelin/capelin.len_mat.agg

;	Growth
;
doesgrow               1
growthfunction   lengthvbsimple
growthparameters       ( * 1 #capelin.linf)  #capelin.k   0.0000009187728 3.598994 
;
;	Growth implementation
; a statistical distribution of actual growth around that mean.
;
beta 	( * 100 #capelin.betabin)
maxlengthgroupgrowth 5; the maximum number of length categories a fish can grow in a single timestep

;weightgrowthfile    capelin.data/capelin.grow.weights


;	Natural mortality
;
;            age    4   5   ;
naturalmortality    0.2 0.9 ;

;	the stock acting as prey
;
iseaten		1  
preylengths	agg/capelin/capelin.len_mat.agg
energycontent	5.3

;	the stock acting as a predator
;
doeseat    0

;	initial conditions of the stock
;
initialconditions 
numbers
minage            4
maxage            5
minlength         0
maxlength         25
dl                0.5; step size for the length groups
initstockfile     capelin/capelin.mat.init

;	migraiton of the stock
;
doesmigrate       0
;yearstepfile 		capelin/caplein.migrate.time
;definematrices 	capelin/capelin.migrate.matrix


;	maturation of the stock
;
doesmature	  0

;	movement (transition) of the stock
;
doesmove          0

;	recruitment of the stock
;
doesrenew         0

;	spawning of the stock
;
doesspawn 	  1
spawnfile 	capelin/capelin.spawn

;	straying of the stock
;
doesstray	0
